{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent":"off",
        "no-tabs": 0,
        "eol-last": 0,
        "no-underscore-dangle": 0,
        "multiline": 0,
        "no-console": "off",
        "no-restricted-globals": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
